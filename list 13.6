#include <iostream>
#include <fstream>
#include <string>
using namespace std;

int main()
{
	//파일열기
	ifstream input("state.txt");

	if (input.fail())
	{
		cout << "File does not exist" << endl;
		cout << "Exit program" << endl;
		return 0;
	}

	//데이터읽기
	string state;

	while (!input.eof()) //파일의 끝이 아니면 계속 실행
	{
		getline(input, state, '#'); //구분문자 #
		cout << state << endl;
	}

	input.close();
	cout << "Done" << endl;

	return 0;
}
